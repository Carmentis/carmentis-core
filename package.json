{
  "name": "@cmts-dev/carmentis-sdk",
  "type": "module",
  "version": "1.17.0",
  "author": "Carmentis SAS",
  "scripts": {
    "clean": "rimraf dist && rimraf types",
    "build:client": "rollup --config rollup.config.client.js",
    "build:server": "rollup --config rollup.config.server.js",
    "build:common-export": "npx barrelsby --delete -d src/common -e test -e Test -e benchmark -e src/bin/carmentis-cli.ts -e node_modules -e spec -n common",
    "build": "npm run build:common-export && tsc && npm run build:client && npm run build:server",
    "docs": "ts-node src/bin/gen-docs.ts",
    "gen:code": "tsx src/bin/generate-sections.ts"
  },
  "bin": {
    "carmentis-cli": "dist/bin/carmentis-cli.js"
  },
  "exports": {
    "./client": {
      "require": "./dist/client/index.cjs",
      "import": "./dist/client/index.mjs",
      "types": "./dist/client/index.d.ts"
    },
    "./server": {
      "require": "./dist/server/index.cjs",
      "import": "./dist/server/index.mjs",
      "types": "./dist/server/index.d.ts"
    }
  },
  "main": "dist/server/index.cjs",
  "module": "dist/server/index.mjs",
  "types": "dist/server/index.d.ts",
  "typesVersions": {
    "*": {
      "client": [
        "dist/client/index.d.ts"
      ],
      "server": [
        "dist/server/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Carmentis/carmentis-core.git"
  },
  "description": "Carmentis SDKs",
  "license": "SEE LICENCE IN LICENCE.txt",
  "bugs": {
    "url": "https://github.com/Carmentis/carmentis-core/issues"
  },
  "homepage": "https://github.com/Carmentis/carmentis-core#readme",
  "devDependencies": {
    "@babel/plugin-syntax-bigint": "^7.8.3",
    "@babel/preset-env": "^7.27.2",
    "@babel/preset-typescript": "^7.27.1",
    "@jest/globals": "^30.0.1",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "@types/eslint-plugin-security": "^3.0.0",
    "@types/jest": "^30.0.0",
    "@typescript-eslint/eslint-plugin": "^8.37.0",
    "@typescript-eslint/parser": "^8.37.0",
    "barrelsby": "^2.8.1",
    "conventional-changelog-eslint": "^6.0.0",
    "eslint-plugin-security": "^3.0.1",
    "jest": "^30.0.1",
    "rimraf": "^6.0.1",
    "rollup": "^4.30.1",
    "rollup-plugin-dts": "^6.1.1",
    "ts-jest": "^29.4.0",
    "ts-morph": "^26.0.0",
    "ts-node": "^10.9.2",
    "tsdoc-markdown": "^1.4.1",
    "tslib": "^2.8.1",
    "tsx": "^4.20.3",
    "typescript": "^5.8.3",
    "underscore": "^1.13.7"
  },
  "dependencies": {
    "@logtape/logtape": "^1.2.0",
    "@noble/ciphers": "^1.2.1",
    "@noble/hashes": "^1.7.1",
    "@noble/post-quantum": "^0.4.1",
    "@noble/secp256k1": "^2.2.3",
    "asn1-ts": "^11.0.1",
    "axios": "^1.10.0",
    "cbor-x": "^1.6.0",
    "clipanion": "^3.2.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "valibot": "^1.2.0"
  }
}
