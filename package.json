{
  "name": "@cmts-dev/carmentis-sdk",
  "type": "module",
  "version": "1.3.0",
  "author": "Carmentis SAS",
  "scripts": {
    "clean": "rimraf dist && rimraf types",
    "build:client": "rollup --config rollup.config.client.js",
    "build:server": "rollup --config rollup.config.server.js",
    "build:application": "rollup --config rollup.config.application.js",
    "build": "tsc && npm run build:client && npm run build:server && npm run build:application"
  },
  "exports": {
    "./client": {
      "require": "./dist/client/index.cjs",
      "import": "./dist/client/index.mjs",
      "types": "./dist/client/index.d.ts"
    },
    "./server": {
      "require": "./dist/server/index.cjs",
      "import": "./dist/server/index.mjs",
      "types": "./dist/server/index.d.ts"
    },
    "./application": {
      "require": "./dist/application/index.cjs",
      "import": "./dist/application/index.mjs",
      "types": "./dist/application/index.d.ts"
    }
  },
  "main": "dist/server/index.cjs",
  "module": "dist/server/index.mjs",
  "types": "dist/server/index.d.ts",
  "typesVersions": {
    "*": {
      "client": [
        "dist/client/index.d.ts"
      ],
      "server": [
        "dist/server/index.d.ts"
      ],
      "application": [
        "dist/application/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Carmentis/carmentis-core.git"
  },
  "description": "Carmentis SDKs",
  "license": "SEE LICENCE IN LICENCE.txt",
  "bugs": {
    "url": "https://github.com/Carmentis/carmentis-core/issues"
  },
  "homepage": "https://github.com/Carmentis/carmentis-core#readme",
  "devDependencies": {
    "@rollup/plugin-commonjs": "^28.0.2",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^16.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.2",
    "rimraf": "^6.0.1",
    "rollup": "^4.30.1",
    "rollup-plugin-dts": "^6.1.1",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3"
  },
  "dependencies": {
    "@noble/ciphers": "^1.2.1",
    "@noble/hashes": "^1.7.1",
    "@noble/secp256k1": "^2.2.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/commit-analyzer": "^13.0.1",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/release-notes-generator": "^14.0.3",
    "conventional-changelog-eslint": "^6.0.0",
    "socket.io-client": "^4.8.1"
  }
}
